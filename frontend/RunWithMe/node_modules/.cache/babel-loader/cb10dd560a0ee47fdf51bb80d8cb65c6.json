n        state.contactList = data.data;\r\n      })\r\n  },\r\n  createAndSelectChatroom : (state) => {\r\n    http\r\n      .post(\"/chat/room\")\r\n      .then((data) =>{\r\n        console.log(data);\r\n      })\r\n  }\r\n};\r\n\r\nexport default {\r\n  state,\r\n  getters,\r\n  actions,\r\n  mutations\r\n};\r\n"]}]}